@article{Bulling2008a,
abstract = {This article is about defining a suitable logic for expressing classical game theoretical notions. We define an extension of alternating-time temporal logic (ATL) that enables us to express various rationality assumptions of intelligent agents. Our proposal, the logic ATLP (ATLwith plausibility) allows us to specify sets of rational strategy profiles in the object language, and reason about agents' play if only these strategy profiles were allowed. For example, we may assume the agents to play only Nash equilibria, Pareto-optimal profiles or undominated strategies, and ask about the resulting behaviour (and outcomes) under such an assumption. The logic also gives rise to generalized versions of classical solution concepts through characterizing patterns of payoffs by suitably parameterized formulae of ATLP. We investigate the complexity of model checking ATLP for several classes of formulae: It ranges from \$\backslash Delta\_\{\backslash mathbf\{3\}\}\^{}\{\backslash mathbf\{P\}\}\$ to PSPACE in the general case and from \$\backslash Delta\_\{\backslash mathbf\{3\}\}\^{}\{\backslash mathbf\{P\}\}\$ to \$\backslash Delta\_\{\backslash mathbf\{4\}\}\^{}\{\backslash mathbf\{P\}\}\$ for the most interesting subclasses, and roughly corresponds to solving extensive games with imperfect information.},
author = {Bulling, Nils and Jamroga, Wojciech and Dix, J\"{u}rgen},
doi = {10.1007/s10472-009-9110-4},
file = {:Users/armando/gdrive/mendeley-content/2008/Bulling, Jamroga, Dix - 2008 - Reasoning about temporal properties of rational play(2).pdf:pdf},
issn = {10122443},
journal = {Annals of Mathematics and Artificial Intelligence},
number = {1-4},
pages = {51--114},
title = {{Reasoning about temporal properties of rational play}},
volume = {53},
year = {2008}
}
