@article{Thesis2011,
abstract = {This thesis looks at the possibilities and problems that arise when implementing Smoothed-Particle Hydrodynamics (SPH) from a Game Programmer’s perspective. It explains how to implement SPH, what formulas to use, and how to get a simulation that looks convincing. After this it explains how to optimize this effectively, using several techniques, including a GPU based approach. Finally, we discuss bottlenecks that one will face when trying to implement a fluid system into a game. As a reference for this we use a game built from scratch that uses fluids as its main gameplay element. A final conclusion is based on the experience of building this game, and a look at industry examples. As is seen in the research and from the industry examples, SPH is still expensive to put into a game, and therefore it is only used in specific instances. However, the great performance results that are obtained by porting the simulation to a current-generation GPU show a bright future.},
author = {Thesis, Specialisation and Ezendam, Roel},
file = {:Users/armando/gdrive/mendeley-content/2011/Thesis, Ezendam - 2011 - A Performance Study on the In-game use of Smoothed-Particle Hydrodynamics between a CPU and GPU Implementation.pdf:pdf},
